{
  "TokenOptions": {
    "Audience": "yourAudience",
    "Issuer": "yourIssuer",
    "AccessTokenExpiration": 60,
    "SecurityKey": "your_secret_key_here"
  },
  "ConnectionStrings": {
    "PostgresSQL": "Host=localhost;Port=5432;Database=yourDb;Username=yourUser;Password=yourPassword;Include Error Detail=true"
  },
  "Smtp": {
    "Host": "smtp.example.com",
    "Port": 587,
    "EnableSsl": true,
    "User": "your_email@example.com",
    "Password": "your_password",
    "SenderName": "YourAppName"
  }
}
